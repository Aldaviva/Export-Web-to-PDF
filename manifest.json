{
 "manifest_version": 2,
 "name": "Export Web to PDF",
 "description": "Exports the current webpage when you click the icon",
 "version": "1.0",
 "icons": {
  "170": "icon.png" },
 "permissions": [
  "tabs" ],
 "background": {
  "scripts": ["background.js"] },
 "browser_action": {
  "default_icon": {
   "170": "icon.png" },
  "default_title": "Export to PDF" }
}

